#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/USUARIO/Fulcrum/Fulcrum/node_modules/.pnpm/tsup@8.5.0_@swc+core@1.12.5_ea7ccf29764c38eae166a800e5005662/node_modules/tsup/dist/node_modules:/mnt/c/Users/USUARIO/Fulcrum/Fulcrum/node_modules/.pnpm/tsup@8.5.0_@swc+core@1.12.5_ea7ccf29764c38eae166a800e5005662/node_modules/tsup/node_modules:/mnt/c/Users/USUARIO/Fulcrum/Fulcrum/node_modules/.pnpm/tsup@8.5.0_@swc+core@1.12.5_ea7ccf29764c38eae166a800e5005662/node_modules:/mnt/c/Users/USUARIO/Fulcrum/Fulcrum/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/USUARIO/Fulcrum/Fulcrum/node_modules/.pnpm/tsup@8.5.0_@swc+core@1.12.5_ea7ccf29764c38eae166a800e5005662/node_modules/tsup/dist/node_modules:/mnt/c/Users/USUARIO/Fulcrum/Fulcrum/node_modules/.pnpm/tsup@8.5.0_@swc+core@1.12.5_ea7ccf29764c38eae166a800e5005662/node_modules/tsup/node_modules:/mnt/c/Users/USUARIO/Fulcrum/Fulcrum/node_modules/.pnpm/tsup@8.5.0_@swc+core@1.12.5_ea7ccf29764c38eae166a800e5005662/node_modules:/mnt/c/Users/USUARIO/Fulcrum/Fulcrum/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli-default.js" "$@"
else
  exec node  "$basedir/../../dist/cli-default.js" "$@"
fi
