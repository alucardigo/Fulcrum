{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests --detectOpenHandles --config jest.config.js",
    "test:unit": "jest --testPathPattern=src/.*\\.spec\\.ts$ --passWithNoTests --detectOpenHandles --config jest.config.js",
    "test:integration": "jest --testPathPattern=test/.*\\.spec\\.ts$ --passWithNoTests --detectOpenHandles --config jest.config.js",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:watch": "jest --watch --config jest.config.js",
    "test:cov": "jest --coverage --config jest.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config jest.config.js",
    "docker:db": "docker-compose up -d database",
    "docker:test-db": "docker-compose -f docker-compose.test.yml up -d test-database",
    "docker:all": "docker-compose up -d",
    "pretest:e2e": "npm run docker:test-db && npm run prisma:generate",
    "db:generate": "prisma generate",
    "db:migrate:dev": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset --force",
    "security:audit": "npm audit && snyk test",
    "validate:all": "npm run lint && npm run test:all && npm run security:audit"
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
